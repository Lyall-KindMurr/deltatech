#. module: deltatech_sale_feedback
#: model:mail.template,subject:deltatech_sale_feedback.mail_template_sale_feedback
msgid "${object.company_id.name} (Ref ${object.name or 'n/a' })"
msgstr ""

#. module: deltatech_sale_feedback
#: model:mail.template,body_html:deltatech_sale_feedback.mail_template_sale_feedback
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"                    Dear\n"
"                    % if object.partner_id.parent_id:\n"
"                    ${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"                    % else:\n"
"                    ${object.partner_id.name},\n"
"                    % endif\n"
"                </p>\n"
"                <p>\n"
"                    Please take a moment to rate our products related to the invoice \"<strong>${object.name}</strong>\"\n"
"                </p>\n"
"                <br>\n"
"                <div style=\"margin: 0px; padding: 0px;\">\n"
"\n"
"                    % for line in object.invoice_line_ids:\n"
"\n"
"                    % if line.display_type in ['line_section', 'line_note']:\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td colspan=\"4\">\n"
"                                % if line.display_type == 'line_section':\n"
"                                <strong>${line.name}</strong>\n"
"                                % elif line.display_type == 'line_note':\n"
"                                <i>${line.name}</i>\n"
"                                % endif\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    % else\n"
"\n"
"                    % set access_token = line.rating_get_access_token()\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td style=\"width: 100px;\">\n"
"                                <img src=\"/web/image/product.product/%24%7Bline.product_id.id%7D/image_128\" style=\"width: 64px; height: 64px; object-fit: contain;\" alt=\"Product image\">\n"
"                            </td>\n"
"                            <td align=\"left\">\n"
"                                <a href=\"%24%7Bline.product_id.website_url%7D\">${line.product_id.name}</a>\n"
"                            </td>\n"
"                            <td width=\"15%\" align=\"center\">${line.product_uom_qty}</td>\n"
"                            <td style=\"font-size: 10px;\">\n"
"                                <table style=\"width:100%;text-align:center;\">\n"
"                                    <tr>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/5\">\n"
"                                                <img alt=\"Satisfied\" src=\"/rating/static/src/img/rating_5.png\" title=\"Satisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/3\">\n"
"                                                <img alt=\"Not satisfied\" src=\"/rating/static/src/img/rating_3.png\" title=\"Not satisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/1\">\n"
"                                                <img alt=\"Highly Dissatisfied\" src=\"/rating/static/src/img/rating_1.png\" title=\"Highly Dissatisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                </table>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    % endif\n"
"                    % endfor\n"
"                </div>\n"
"\n"
"\n"
"            </div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"                    Dragă\n"
"                    % if object.partner_id.parent_id:\n"
"                    ${object.partner_id.name} (${object.partner_id.parent_id.name}),\n"
"                    % else:\n"
"                    ${object.partner_id.name},\n"
"                    % endif\n"
"                </p>\n"
"                <p>\n"
"                    Vă rugăm să acordati un moment pentru a evalua produsele noastre legate de factura \"<strong>${object.name}</strong>\"\n"
"                </p>\n"
"                <br>\n"
"                <div style=\"margin: 0px; padding: 0px;\">\n"
"\n"
"                    % for line in object.invoice_line_ids:\n"
"\n"
"                    % if line.display_type in ['line_section', 'line_note']:\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td colspan=\"4\">\n"
"                                % if line.display_type == 'line_section':\n"
"                                <strong>${line.name}</strong>\n"
"                                % elif line.display_type == 'line_note':\n"
"                                <i>${line.name}</i>\n"
"                                % endif\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    % else\n"
"\n"
"                    % set access_token = line.rating_get_access_token()\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td style=\"width: 100px;\">\n"
"                                <img src=\"/web/image/product.product/%24%7Bline.product_id.id%7D/image_128\" style=\"width: 64px; height: 64px; object-fit: contain;\" alt=\"Product image\">\n"
"                            </td>\n"
"                            <td align=\"left\">\n"
"                                <a href=\"%24%7Bline.product_id.website_url%7D\">${line.product_id.name}</a>\n"
"                            </td>\n"
"                            <td width=\"15%\" align=\"center\">${line.product_uom_qty}</td>\n"
"                            <td style=\"font-size: 10px;\">\n"
"                                <table style=\"width:100%;text-align:center;\">\n"
"                                    <tr>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/5\">\n"
"                                                <img alt=\"Satisfied\" src=\"/rating/static/src/img/rating_5.png\" title=\"Satisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/3\">\n"
"                                                <img alt=\"Not satisfied\" src=\"/rating/static/src/img/rating_3.png\" title=\"Not satisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                        <td>\n"
"                                            <a href=\"/rate/%24%7Baccess_token%7D/1\">\n"
"                                                <img alt=\"Highly Dissatisfied\" src=\"/rating/static/src/img/rating_1.png\" title=\"Highly Dissatisfied\">\n"
"                                            </a>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                </table>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    % endif\n"
"                    % endfor\n"
"                </div>\n"
"\n"
"\n"
"            </div>\n"
"        "


